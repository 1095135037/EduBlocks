{
    "name": "edublocks-ui",
    "version": "2.0.0",
    "description": "EduBlocks UI",
    "scripts": {
        "build": "tsc && browserify build/main.js -o dist/main.js",
        "watch": "parallel watchify tsc-watch",
        "tsc-watch": "tsc --watch",
        "watchify": "sleep 5 && watchify build/main.js -o dist/main.js",
        "lint": "tslint src/**/*.ts"
    },
    "repository": "https://github.com/AllAboutCode/EduBlocks",
    "keywords": [
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "AllAboutCode",
    "license": "GitHub",
    "devDependencies": {
        "browserify": "^14.3.0",
        "npm-parallel": "^2.0.1",
        "tslint": "^5.0.0",
        "typescript": "^2.3.1",
        "watchify": "^3.9.0"
    },
    "dependencies": {
        "@types/es6-promise": "0.0.32",
        "@types/file-saver": "^0.0.1",
        "@types/lodash": "^4.14.62",
        "@types/node": "^7.0.12",
        "file-saver": "^1.3.3",
        "lodash": "^4.17.4",
        "picnic": "^6.3.2",
        "preact": "^8.1.0",
        "term.js": "^0.0.7"
    }
}
